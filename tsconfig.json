{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/error.jsx",
    "src/components/SignatureSelects/Selects.jsx",
    "src/components/Footer/footer.jsx",
    "src/components/Navbar/navbar.jsx",
    "src/components/Navbar/Links/navLink/navLink.jsx",
    "src/components/Navbar/Links/Links.jsx",
    "src/app/page.tsx",
    "src/app/layout.tsx",
    "src/lib/action.js",
    "src/lib/data.ts",
    "src/lib/auth.js",
    "src/lib/database.ts",
    "src/lib/models.js"
, "src/app/api/webhooks/route.js"  ],
  "exclude": ["node_modules"]
}
